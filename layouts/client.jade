            script(src='/components/sass-bootstrap/dist/js/bootstrap.min.js')

            script(src='/socket.io/socket.io.js')


            script(src='/components/quojs/quo.js')
            script(src='/components/quojs/quo.ajax.js')
            script(src='/components/quojs/quo.css.js')
            script(src='/components/quojs/quo.element.js')
            script(src='/components/quojs/quo.environment.js')
            script(src='/components/quojs/quo.events.js')
            script(src='/components/quojs/quo.gestures.js')
            script(src='/components/quojs/quo.output.js')
            script(src='/components/quojs/quo.query.js')


            script(src='/js/client/bootstrap-slider.js')
            script(src='/components/sass-bootstrap/js/modal.js')
            script(src='/js/client/client.js')
    body

        div.remote-control
